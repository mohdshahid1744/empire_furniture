			<style>


.hero__items {
  position: relative;
  background-size: cover;
  background-position: center;
  height: 500px;
  transition: height 0.3s ease;
  overflow: hidden;
  border-radius: 10px;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3); 
}

.overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.3)); 
  transition: background 0.3s ease;
}

.hero__items:hover .overlay {
  background: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0)); 
}

.hero__text {
  position: relative;
  z-index: 1; 
}

.text-shadow {
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5); 
}
.hero__social {
  margin-top: 20px;
}

.social-icon {
  font-size: 24px;
  margin-right: 10px;
  color: #fff;
  transition: color 0.3s ease;
}

.social-icon:hover {
  color: #ffc107; 
}


.stylish-title {
  font-size: 28px;
  color: #ffc107;
  letter-spacing: 1px;
}

.stylish-description {
  font-size: 36px;
  color: #fff;
  letter-spacing: 1.5px;
}

.stylish-coupon {
  font-size: 18px;
  color: #4caf50;
  letter-spacing: 1px;
}

@media (max-width: 767px) {
 
  .stylish-title {
    font-size: 24px;
  }

  .stylish-description {
    font-size: 15px;
  }
  .stylish-coupon {
    font-size: 16px;
  }
  .stylish-button {
      font-size: 12px; 
      padding: 4px 10px; 
    }
  
}

@media (max-width: 767px) {
   .hero__items {
	  height: 200px; 
	}
}
@media (max-width: 767px) { 
    .img-small {
        max-width: 50%; 
        height:290px; 
    }
}

